  pod 'FlareLane', :path => '../'
end

target 'FlareLane_Example' do
  example_pods

  target 'FlareLane_Tests' do
    inherit! :search_paths
  end
end

target 'FlareLaneNotificationServiceExtension' do
  example_pods
end
